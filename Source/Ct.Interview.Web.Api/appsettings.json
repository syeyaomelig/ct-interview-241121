{
    "ServerAddress": "https://localhost:7143",
    "IpRateLimiting": {
        "EnableEndpointRateLimiting": false,
        "StackBlockedRequests": true,
        "RealIpHeader": "X-Real-IP",
        "ClientIdHeader": "X-ClientId",
        "HttpStatusCode": 429,
        "IpWhitelist": [ ],
        "EndpointWhitelist": [ ],
        "ClientWhitelist": [ ],
        "GeneralRules": [
            {
                "Endpoint": "*",
                "Period": "1s",
                "Limit": 1 // 100 calls/s
            },
            {
                "Endpoint": "*",
                "Period": "15m",
                "Limit": 1 // 10,000 in a 15-minute window.
            }
        ]
    }
}
